{
  "name": "Page Edit",
  "description": "An visual editor for HTML elements",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["tabs", "<all_urls>", "storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Page Edit",
    "default_popup": "popup/popup.html"
  }
}